<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:background="@color/colorPrimaryDark"
                                             android:layout_width="match_parent"
                                             android:layout_height="wrap_content" android:layout_marginBottom="10dp">
    <com.google.android.material.card.MaterialCardView android:layout_width="411dp"
                                        android:layout_height="261dp" android:paddingBottom="19dp"
                                        app:cardBackgroundColor="@color/colorPrimary" app:cardCornerRadius="25dp"
                                        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                                        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                                        android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent" android:layout_marginBottom="12dp"
                android:layout_height="match_parent">

            <TextView
                    android:text="TextView" android:gravity="right" android:paddingRight="10dp"
                    android:paddingTop="10sp"
                    android:layout_width="104dp"
                    android:layout_height="43dp" android:id="@+id/dischargevalue"
                    android:background="@drawable/shape2"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="68dp"
                    app:layout_constraintTop_toTopOf="@+id/textView15" android:textSize="18sp"/>
            <TextView
                    android:text="TextView" android:gravity="right" android:paddingRight="10dp"
                    android:paddingTop="10sp"
                    android:layout_width="103dp"
                    android:layout_height="42dp" android:id="@+id/deathsvalue"
                    android:background="@drawable/shape2"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="72dp"
                    app:layout_constraintTop_toTopOf="@+id/textView13" android:textSize="18sp"/>
            <TextView
                    android:text="TextView" android:gravity="right" android:paddingRight="10dp"
                    android:paddingTop="10sp"
                    android:layout_width="101dp"
                    android:layout_height="42dp" android:id="@+id/onadmisioncasevalue"
                    android:background="@drawable/shape2"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="72dp"
                    app:layout_constraintTop_toTopOf="@+id/textView5" android:layout_marginTop="4dp"
                    android:textSize="18sp"/>
            <TextView
                    android:text="TextView" android:gravity="right" android:paddingRight="10dp"
                    android:paddingTop="10sp"
                    android:layout_width="100dp"
                    android:layout_height="43dp" android:id="@+id/labconfirmvalue"
                    android:background="@drawable/shape2"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="72dp"
                    android:layout_marginTop="48dp" app:layout_constraintTop_toTopOf="parent" android:textSize="20sp"/>
            <TextView
                    android:text="Discharge" android:gravity="center"
                    android:layout_width="208dp" android:textColor="@color/colorAccent"
                    android:layout_height="43dp" android:id="@+id/textView15"
                    android:background="@drawable/shape4" app:layout_constraintEnd_toEndOf="@+id/dischargevalue"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.388"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView13"/>
            <TextView
                    android:text="STATE"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/stateid"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:textColor="@color/colorAccent" android:gravity="center" android:textStyle="bold"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:textSize="18sp"/>
            <TextView
                    android:text="Deaths"
                    android:layout_width="208dp" android:textColor="@color/colorAccent"
                    android:layout_height="43dp" android:id="@+id/textView13"
                    android:background="@drawable/shape6" android:gravity="center" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="@+id/deathsvalue" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.388"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView5"/>
            <TextView
                    android:text="Lab Confirmed Cases" android:gravity="center"
                    android:layout_width="208dp" android:textColor="@color/colorAccent"
                    android:layout_height="43dp" android:id="@+id/textView12"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"
                    android:background="@drawable/shape"
                    android:layout_marginTop="48dp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.034"/>
            <TextView
                    android:text="On addmission cases" android:gravity="center"
                    android:layout_width="208dp" android:textColor="@color/colorAccent"
                    android:layout_height="43dp" android:id="@+id/textView5"
                    android:background="@drawable/shape5"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView12"
                    android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/onadmisioncasevalue"
                    android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.388"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>